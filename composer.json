{
  "name": "premierstacks/php-app-template",
  "description": "premierstacks php app template",
  "type": "project",
  "keywords": [
    "tomchochola",
    "premierstacks",
    "php-app-template"
  ],
  "homepage": "https://github.com/premierstacks/php-app-template",
  "readme": "./README.md",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com",
      "homepage": "https://github.com/tomchochola",
      "role": "Author"
    }
  ],
  "support": {
    "email": "chocholatom1997@gmail.com",
    "issues": "https://github.com/premierstacks/php-app-template/issues",
    "source": "https://github.com/premierstacks/php-app-template.git",
    "docs": "https://github.com/premierstacks/php-app-template"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/tomchochola"
    }
  ],
  "require": {
    "php": "8.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "premierstacks/php-cs-fixer-stack": "@dev",
    "premierstacks/phpstan-stack": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Premierstacks\\PhpTemplate\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/premierstacks/php-cs-fixer-stack.git"
    },
    {
      "type": "git",
      "url": "https://github.com/premierstacks/phpstan-stack.git"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "preferred-install": "dist",
    "audit": {
      "abandoned": "fail"
    },
    "optimize-autoloader": true,
    "platform-check": true
  }
}
